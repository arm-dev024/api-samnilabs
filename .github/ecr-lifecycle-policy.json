{
  "rules": [
    {
      "rulePriority": 1,
      "description": "Keep last 1 buildcache image for Docker layer caching",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": ["buildcache"],
        "countType": "imageCountMoreThan",
        "countNumber": 1
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 2,
      "description": "Keep last 3 application images for rollback",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": ["latest"],
        "countType": "imageCountMoreThan",
        "countNumber": 3
      },
      "action": {
        "type": "expire"
      }
    }
  ]
}
